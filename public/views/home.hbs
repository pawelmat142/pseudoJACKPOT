	<link rel="apple-touch-icon" sizes="180x180" href="img/favicon/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="img/favicon/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="img/favicon/favicon-16x16.png">
	<link rel="manifest" href="img/favicon/site.webmanifest">

    {{!-- styles --}}
    <link rel="stylesheet" type="text/css" href="css/font/flaticon.css">
    <link rel="stylesheet" type="text/css" href="css/animate.css">
    <link rel="stylesheet" type="text/css" href="css/styles.css">
    <link rel="stylesheet" type="text/css" href="css/game-board.css">
    <link rel="stylesheet" type="text/css" href="css/game-ui.css">
    <link rel="stylesheet" type="text/css" href="css/audio-manager.css">
    <link rel="stylesheet" type="text/css" href="css/modal.css">
</head>

<body>

    {{!-- <div class="img">
        <img src="../img/board.png" alt="bb">
    </div> --}}

    <div id="game-board">

        {{#each columns as |item|}}
            <div class="column-wrapper"><div class="column">{{#each item}}<div class="item-wrapper"></div>{{/each}}</div></div>
        {{/each}}

        <div class="xxx">
            {{#each columns}}
                <div class="line"><span class="left">1</span><span class="right">2</span></div>
            {{/each}}
            <div class="line x"><span class="left">1</span><span class="right">2</span></div>
            <div class="line x"><span class="left">1</span><span class="right">2</span></div>
        </div>

    </div>

    <div id="game-ui">

        {{!-- display --}}
        <div class="display">

            <div>
                <p id="coins">555555555</p>
                <p class="label">COINS</p>
            </div>


            <div class="bet">
                <i id="betDown" class="flaticon-minus"></i>
                <div>
                    <p id="bet">555</p> 
                    <p class="label">BET</p>
                </div>
                <i id="betUp" class="flaticon-plus"></i>
            </div>

            <div>
                <p id="win">555555</p>
                <p class="label">WIN</p>
            </div>

        </div>



        {{!-- controls --}}
        <div class="buttons">
            <div class="btn blue" id="scores"><span><p>Scores</p></span></div>
            <div class="btn" id="reset"><span><p>RESET</p></span></div>
            <div class="btn blue" id="autoplay"><span><p>AUTOPLAY</p></span></div>
            <div class="btn blue" id="transfer"><span><p>TRANSFER</p></span></div>
            <div class="btn green" id="spin"><span><p>SPIN</p></span></div>
        </div>



    </div>


    {{!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script> --}}
